<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/signinFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.kusitms.hdmedi.ui.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/medicineFragment"
        android:name="com.kusitms.hdmedi.ui.MedicineFragment"
        android:label="MedicineFragment"
        tools:layout="@layout/fragment_medicine" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.kusitms.hdmedi.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/selfCheckFragment"
        android:name="com.kusitms.hdmedi.feature.selfcheck.ui.SelfCheckFragment"
        android:label="SelfCheckFragment"
        tools:layout="@layout/fragment_self_check" />

    <fragment
        android:id="@+id/mypageFragment"
        android:name="com.kusitms.hdmedi.MypageFragment"
        android:label="MypageFragment"
        tools:layout="@layout/fragment_mypage" />

    <fragment
        android:id="@+id/signinFragment"
        android:name="com.kusitms.hdmedi.feature.signin.ui.SigninFragment"
        android:label="SigninFragment"
        tools:layout="@layout/fragment_signin">
        <action
            android:id="@+id/action_signinFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
